{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "activities.html", "use": "@vercel/static" },
    { "src": "about.html", "use": "@vercel/static" },
    { "src": "quiz.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/activities", "dest": "/activities.html" },
    { "src": "/about", "dest": "/about.html" },
    { "src": "/quiz", "dest": "/quiz.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
